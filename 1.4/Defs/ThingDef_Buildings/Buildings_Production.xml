<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>VNPEA_NutrientPasteTapI</defName>
    <label>improved nutrient paste tap</label>
    <description>An upgraded version of the nutrient paste tap.</description>
    <thingClass>VNPEA.Building_NutrientPasteTapI</thingClass>
    <graphicData>
      <texPath>Things/Building/NutrientPasteTap/NutrientPasteTap</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <color>(192,136,220)</color>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(4,2)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <statBases>
      <MaxHitPoints>275</MaxHitPoints>
      <WorkToBuild>3500</WorkToBuild>
      <Flammability>0.4</Flammability>
    </statBases>
    <size>(3,1)</size>
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <building>
      <isMealSource>true</isMealSource>
      <wantsHopperAdjacent>false</wantsHopperAdjacent>
      <soundDispense>DispensePaste</soundDispense>
      <forceShowRoomStats>true</forceShowRoomStats>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <comps>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VNPE_NutrientPasteNet</pipeNet>
      </li>
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>250</basePowerConsumption>
      </li>
    </comps>
    <socialPropernessMatters>true</socialPropernessMatters>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>ImprovedNutrientPaste</li>
    </researchPrerequisites>
  </ThingDef>

